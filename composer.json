{
    "name": "somrlik/hello-bank-cz-api",
    "description": "An implementation of Hello Bank! by Cetelem API for displaying calculators on eshops",
    "keywords": ["api", "hello-bank", "cz", "cetelem"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Karel 'Somrlik' Syrovy",
            "email": "somrlik@gmail.com",
            "homepage": "https://somrlik.cz"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "ext-SimpleXML": "^7.0",
        "ext-json": "^7.0",
        "psr/http-factory": "^1.0",
        "nette/utils": "~2.2, >=2.2.2",
        "php-http/httplug": "^1.1",
        "php-http/discovery": "^1.4",
        "php-http/message": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "guzzlehttp/guzzle": "^6.3",
        "php-http/guzzle6-adapter": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Somrlik\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Somrlik\\Tests\\": "tests/"
        }
    }
}
