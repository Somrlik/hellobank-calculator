{
    "name": "somrlik/hello-bank-cz-api",
    "description": "An implementation of Hello Bank! by Cetelem API for displaying calculators on eshops",
    "keywords": ["api", "hello-bank", "cz", "cetelem"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Karel 'Somrlik' Syrovy",
            "email": "somrlik@gmail.com",
            "homepage": "https://somrlik.cz"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0",
        "ext-SimpleXML": "^7.0",
        "psr/http-factory": "^1.0@dev",
        "nette/utils": "^3.0@dev",
        "php-http/httplug": "^1.2@dev",
        "php-http/discovery": "^1.3@dev",
        "php-http/message": "^1.6@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.35 || ~5.7 || ~6.4 || ~7.0",
        "guzzlehttp/guzzle": "~6.0",
        "php-http/guzzle6-adapter": "^1.2@dev"
    },
    "autoload": {
        "psr-4": {
            "Somrlik\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Somrlik\\Tests\\": "tests/"
        }
    }
}
